@page
@model CacheTestModel
@{
    ViewData["Title"] = "Cache Test";
}

<h2>Cache Test</h2>
<p>Display the keys of the data currently stored in memorycache. For testing purposes.</p>

@if (Model.CacheKeys.Any())
{
    <ul>
        @foreach (var key in Model.CacheKeys)
        {
            <li>@key</li>
        }
    </ul>
}
else
{
    <p>No cache entries found.</p>
}
